package com.morningstar;

import static org.junit.Assert.*;

import org.junit.After;
import org.junit.Before;
import org.junit.Test;

public class StringManipulationTest {
	
	 StringManipulation target = null;
	
	@Before
	public void setup() {
		target = new StringManipulation();
	}
	
	@After
	public void teardown() {
		target=null;
	}

	@Test
	public void testDoOperation() {
		assertEquals(null,target.doOperation("Mahesh","Akshay","Ankita"));
	}
	
	@Test
	public void testDoOperation1() {
		assertEquals("mahesh3kad", target.doOperation1("Mahesh", 3, "kad"));
	}

}
